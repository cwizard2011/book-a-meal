  "main": "server/app.js",
  "dependencies": {
    "acl": "^0.4.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "sequelize-attribute-roles": "0.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^22.4.3",
    "istanbul": "^0.4.5",
    "jest": "^22.4.3",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "should": "^13.2.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "start": "babel-node ./server/app.js",
    "start:dev": "nodemon --watch app --exec babel-node server/app.js",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov mocha --reporter spec --require babel-register server/tests/**/*.js --timeout 600000 --exit",
    "coveralls": "npm run coverage -- --report lcovonly && cat coverage/lcov.info | coveralls",
    "babel-node": "babel-node --presets=latest",
    "test": "mocha --require babel-register server/tests/**/*.js --timeout 100000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwizard2011/book-a-meal.git"
