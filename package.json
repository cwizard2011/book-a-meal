{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "An app that allow a customer view meal of the day, place and order and the caterer to add meal and view order history",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "eslint": "^4.19.1",
    "expect": "^22.4.3",
    "express": "^4.16.3",
    "express-validator": "^5.1.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "scripts": {
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "coverage": "node node_modules/.bin/istanbul cover _mocha -- -R spec",
    "babel-node": "babel-node --presets=latest",
    "start": "babel-node server/app",
    "test": "mocha --require babel-register server/tests/**/*.js --timeout 100000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwizard2011/book-a-meal.git"
  },
